{
  "name": "bitflyer_data_getter", // この関数名で作成or上書き
  "description": "bitflyer のデータを S3 へ保存する",
  "region": "ap-northeast-1",
  "runtime": "python3.7",
  "handler": "lambda_function.lambda_handler",
  "role": "arn:…/service-role/bitflyer_data_getter_role",
  "timeout": 300,
  "memory": 128,
  "requirements": [
    "aioboto3",
    "ccxt",
    "python-dateutil",
    "requests",
    "tzlocal",
    "uvloop"
  ],
  "variables": {
    // ローカルテスト時は、ターミナル内で環境変数を export コマンドなどで設定しておく
    "TZ": "Asia/Tokyo",
    "DISCORD_POST_URL": "https://discordapp.com/api/webhooks/…",
    "S3_BUCKET_NAME": "保存する S3 バケット名"
  }
}
